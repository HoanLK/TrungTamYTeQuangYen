</head>
<body ng-app="front" class="cbp-spmenu-push">
    <div class="col-md-12 col-xs-12 col-sm-12 lienHe">
        <div class="col-md-6 col-xs-6 col-sm-12 pull-left">
            <p class="diaChi"><i class="fa fa-street-view" style="color:#f44336;"></i> Tiền An, Quảng Yên, Quảng Ninh - <span style="color:#f44336;">Hotline: <a href="tel:">...</a></span></p>
        </div>
        <div class="col-md-6 col-xs-6 col-sm-12 right">
            <p class="pull-right">
                <a href="/lien-he" class="thongTin">Liên hệ</a>
                <a href="/video" class="thongTin">Video</a>
                <a href="/bac-si" class="thongTinFB"><i class="fa fa-facebook-f" style="padding:1em 1.2em;"></i></a>
                <a href="/bac-si" class="thongTinYT"><i class="fa fa-youtube-play" style="padding:1em;"></i></a>
            </p>
        </div>
    </div>
    <div class="col-md-12 col-xs-12 col-sm-12" style="background: url(../../Content/Front/images/nen.jpg)  no-repeat center; background-size: cover;">
        <div class="col-md-2 col-xs-12 col-sm-2 divLogo">
            <img src="~/Content/Front/images/logo.jpg" class="img-responsive" />
        </div>
        <div class="col-md-10 col-xs-12 col-sm-10 pull-right tieuDeHeader">
            <p class="tenBenhVien text-uppercase">Trung tâm y tế thị xã quảng yên</p>
            <p class="luongY">"Lương y như từ mẫu"</p>
        </div>
    </div>
    <div class="head_top header_admin">
        <div class="bg_top">
            <script>
                function jsupdate(k, idclass, jscolor) {
                    if (k == 0) $("." + idclass).css("background-color", "#" + jscolor);
                    else if (k == 1) $("." + idclass).css("color", "#" + jscolor);
                }
            </script>
        </div>
    </div>
    <div class="col-md-12 col-xs-12 col-sm-12 header-bottom" ng-controller="homeController">
        <div class="container">
            <nav class="navbar navbar-default" role="navigation">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <!--/.navbar-header-->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li class="">
                            <a href="@Url.Action("Index","Home")">trang chủ</a>
                        </li>
                        <li class="menu1">
                            <a href="/gioi-thieu/gioi-thieu-6" class="scroll">giới thiệu</a>
                            <ul class="menu2">
                                <li ng-repeat="(index, value) in gioiThieus"><a href="/gioi-thieu/{{value.alias}}-{{value.id}}">{{value.title}}</a></li>
                            </ul>
                        </li>
                        <li class="">
                            <a href="/dich-vu/dich-vu-13">dịch vụ</a>
                        </li>
                        <li class="">
                            <a href="/tin-tuc">tin tức</a>
                        </li>
                        <li class=""><a class="scroll" href="/thu-vien-anh">Thư Viện Ảnh</a></li>
                        <li class=""><a class="scroll" href="/bac-si">Đội Ngũ Bác Sĩ</a></li>
                        <li class=""><a class="scroll" href="/lien-he">liên hệ</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    <div class="col-md-12 col-xs-12 col-sm-12 banner">
        <section class="slider container" style="margin:0;width:100%;">
            <div class="flexslider">

                <div class="flex-viewport" style="overflow: hidden; position: relative;">
                    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" ng-controller="bannerController">
                        <!-- Indicators -->
                        <ol class="carousel-indicators">
                            <li data-target="#carousel-example-generic" data-slide-to="{{index}}" ng-class="{'active':index==0}" ng-repeat="(index, value) in banners"></li>
                        </ol>

                        <!-- Wrapper for slides -->
                        <div class="carousel-inner" role="listbox">
                            <div class="item " ng-class="{'active':index==0}" ng-repeat="(index, value) in banners">
                                <img ng-src="{{value.image}}" class="img-responsive bannerMain" style="width:100%;" />
                                <div class="carousel-caption bannerText">
                                    <a href="{{value.link}}" class="bannerTitle" style="margin-bottom:0.5em;">{{value.title}}</a>
                                    <p class="bannerContent">{{value.description}}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!-- <div id="conten"> -->
    <article class="col-md-12 col-xs-12 col-sm-12" style="padding:0;">
        @RenderBody()
    </article>
    <!-- </div> -->

    <div class="col-md-12 col-xs-12 col-sm-12 footer" id="contact" style="padding:2em 0 0 0;">
        <div class="container">
            <h6>LIÊN HỆ</h6>
            <div class="co-ntact">
                <div class="contact-top contact-top-right">

                    <div class="map">
                        <a href="/lien-He"><img src="~/Content/Front/images/Dia chi.PNG" class="img-responsive" /></a>
                    </div>
                    <div class="clearfix"> </div>
                </div>
            </div>

            <div class="copyright">
                <p><span class="footer_copyright"><a target="_blank" href="https://www.facebook.com/KetnoitreJsc" alt="Thiết Kế Website" title="Thiết Kế Website" class="a_copyright">Thiết kế bởi Kết Nối Trẻ</a></span></p>
            </div>
        </div>
    </div>

    <a href="#" id="toTop">To Top</a>

    @*<a id="back-to-top" href="#" style="display:block; position:fixed; overflow:hidden; right:5%; bottom:5%; z-index:9999;" class="btn btn-primary btn-lg back-to-top" role="button" data-toggle="tooltip" data-placement="left"><span class="glyphicon glyphicon-chevron-up"></span></a>*@




